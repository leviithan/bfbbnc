# Clone o repositório que você criou
git clone https://github.com/seu-usuario/seu-repositorio.git

# Navegue até o diretório do repositório
cd seu-repositorio

# Crie um novo arquivo de código, por exemplo, um script em Python
echo 'print("Olá, GitHub!")' > hello.py

# Adicione o arquivo ao repositório
git add hello.py

# Faça um commit das mudanças
git commit -m "Adiciona script hello.py"

# Envie as mudanças para o GitHub
git push origin main
